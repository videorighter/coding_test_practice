   "source": [
    "\n",
    "n, m = input[0], input[1]\n",
    "piz = 0\n",
    "cou = n*m\n",
    "while True:\n",
    "    tmp = cou//m\n",
    "    piz += tmp\n",
    "    cou = cou%m\n",
    "    cou += tmp\n",
    "    if cou < m:\n",
    "        break\n",
    "\n",
    "print(piz)"
   ]
