    "print(n)\n",
    "\n",
    "def ubak(n):\n",
    "    if n == 1:\n",
    "        return\n",
    "    else:\n",
    "        if n%2 == 1:\n",
    "            print(int(3*n+1))\n",
    "            return ubak(3*n+1)\n",
    "        else:\n",
    "            print(int(n/2))\n",
    "            return ubak(n/2)\n",
    "\n",
