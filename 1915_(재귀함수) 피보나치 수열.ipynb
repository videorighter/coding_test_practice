    "input_ = int(input())\n",
    "\n",
    "def pivonachi(num):\n",
    "    if num <= 2:\n",
    "        return 1\n",
    "    else:\n",
    "        return pivonachi(num-1)+pivonachi(num-2)\n",
    "    \n",
    "result = pivonachi(input_)\n",
    "print(result)"
